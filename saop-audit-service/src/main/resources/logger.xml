<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%-5level]%d{yyyy-MM-dd HH:mm:ss.SSS} [%thread] %logger [%L] - %msg%n</pattern>
		</encoder>
	</appender>
	<logger name="com.asiainfo" level="error"/>
	<logger name="com.al.crm.nosql.cache.impl.RedisCache" level="info"/>
	<logger name="com.asiainfo.angel.persistence" level="info"/>
	<logger name="org.eclipse.jetty.server.Server" level="info"/>
	<logger name="org.springframework" level="debug"/>
	<logger name="org.eclipse.jetty" level="info"/>
	<logger name="org.springframework.jdbc.datasource.init.ScriptUtils" level="DEBUG"/>

	<logger name="studio.raptor" level="debug"/>

	<root level="error">
		<appender-ref ref="STDOUT"/>
	</root>
</configuration>
